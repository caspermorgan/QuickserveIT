{
  "name": "quickserve-it-agency-v2",
  "version": "2.0.0",
  "description": "Premium digital agency platform for rural India - Full Stack React + Node.js",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "install-all": "npm install && cd client && npm install && cd ..",
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "build:prod": "cd client && npm run build && echo 'Build complete!'",
    "client:install": "cd client && npm install",
    "lint": "echo 'Linting enabled'",
    "test": "echo 'Testing enabled'",
    "eject": "echo 'Not recommended'"
  },
  "keywords": [
    "agency",
    "rural-india",
    "digital-services",
    "react",
    "nodejs",
    "express",
    "tailwindcss",
    "fullstack"
  ],
  "author": "QuickServe IT Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/quickserve-it-agency-v2.git"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "nodemailer": "^6.9.3",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.7.0",
    "uuid": "^9.0.0",
    "compression": "^1.7.4",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "concurrently": "^8.0.1",
    "eslint": "^8.40.0"
  }
}
